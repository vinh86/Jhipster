application {
  config {
    baseName jhipster
    applicationType monolith
    clientFramework react
    authenticationType jwt
    prodDatabaseType postgresql
    devDatabaseType h2Disk
    buildTool maven
  }

  entities *
  dto * with mapstruct
}

entity Jhipster {
  name String
  closed Boolean
}

entity JhipsterSetting {
  theme String
  pageSize Integer
  others String
}

relationship OneToOne {
  JhipsterSetting{jhipster required} to Jhipster
}


